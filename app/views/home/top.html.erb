<div class="main posts-index">
  <div class="container">
    <% @posts.each do |post| %>
      <div class="posts-index-item">
        <!-- 以下をlink_toメソッドを用いて書き換えてください -->
        <%= link_to(post.content, "/home/#{post.id}") %>
      </div>
    <% end %>
    <%= link_to("新規投稿", "/home/new") %>
  </div>
</div>